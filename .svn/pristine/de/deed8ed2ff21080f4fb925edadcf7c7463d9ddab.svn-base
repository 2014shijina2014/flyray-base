<!DOCTYPE html>
<html>
<head>
<title>机构部门管理</title>
#parse("sys/header.html")
</head>
<body>
<div id="rrapp" v-cloak>
	<div v-show="showList">
		<div class="grid-btn">
			<div class="form-group col-sm-2">
				<input type="text" class="form-control" v-model="q.roleName" @keyup.enter="query" placeholder="角色名称">
			</div>
			<a class="btn btn-default" @click="query">查询</a>
			
			#if($shiro.hasPermission("sys:org:save"))
			<a class="btn btn-primary" @click="add"><i class="fa fa-plus"></i>&nbsp;新增</a>
			#end
			#if($shiro.hasPermission("sys:org:update"))
			<a class="btn btn-primary" @click="update"><i class="fa fa-pencil-square-o"></i>&nbsp;修改</a>
			#end
			#if($shiro.hasPermission("sys:org:delete"))
			<a class="btn btn-primary" @click="del"><i class="fa fa-trash-o"></i>&nbsp;删除</a>
			#end
		</div>
	    <table id="jqGrid"></table>
	    <div id="jqGridPager"></div>
    </div>
    
    <div v-show="!showList" class="panel panel-default">
		<div class="panel-heading">{{title}}</div>
		<form class="form-horizontal">
			<div class="form-group">
			   	<div class="col-sm-2 control-label">机构部门编号</div>
			   	<div class="col-sm-10">
			      <input type="text" class="form-control" v-model="org.orgNo" placeholder="机构部门编号"/>
			    </div>
			</div>
			<div class="form-group">
			   	<div class="col-sm-2 control-label">机构部门名称</div>
			   	<div class="col-sm-10">
			      <input type="text" class="form-control" v-model="org.orgName" placeholder="机构部门名称"/>
			    </div>
			</div>
			<div class="form-group">
			   	<div class="col-sm-2 control-label">上级机构部门</div>
			   	<div class="col-sm-10">
			      <input type="text" class="form-control" v-model="org.parentId" placeholder="上级机构部门"/>
			    </div>
			</div>
			<div class="form-group">
			   	<div class="col-sm-2 control-label">创建人</div>
			   	<div class="col-sm-10">
			      <input type="text" class="form-control" v-model="org.createBys" placeholder="创建人"/>
			    </div>
			</div>
			<div class="form-group">
			   	<div class="col-sm-2 control-label">创建时间</div>
			   	<div class="col-sm-10">
			      <input type="text" class="form-control" v-model="org.createTime" placeholder="创建时间"/>
			    </div>
			</div>
			<div class="form-group">
			   	<div class="col-sm-2 control-label">最后更新人</div>
			   	<div class="col-sm-10">
			      <input type="text" class="form-control" v-model="org.lastUpdateBy" placeholder="最后更新人"/>
			    </div>
			</div>
			<div class="form-group">
			   	<div class="col-sm-2 control-label">最后更新时间</div>
			   	<div class="col-sm-10">
			      <input type="text" class="form-control" v-model="org.lastUpdateTime" placeholder="最后更新时间"/>
			    </div>
			</div>
			<div class="form-group">
			   	<div class="col-sm-2 control-label">机构部门排序</div>
			   	<div class="col-sm-10">
			      <input type="text" class="form-control" v-model="org.orgLevel" placeholder="机构部门排序"/>
			    </div>
			</div>
			<div class="form-group">
				<div class="col-sm-2 control-label"></div> 
				<input type="button" class="btn btn-primary" @click="saveOrUpdate" value="确定"/>
				&nbsp;&nbsp;<input type="button" class="btn btn-warning" @click="reload" value="返回"/>
			</div>
		</form>
	</div>
</div>
   
<script src="${rc.contextPath}/js/sys/org.js?_${date.systemTime}"></script>    
</body>
</html>