<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="me.flyray.cms.dao.CustomerProficientDao">

	<select id="queryObject" resultType="me.flyray.cms.model.CustomerProficient">
		select * from crm_customer_proficient where customer_no = #{customerNo}
	</select>


	<select id="queryList" resultType="me.flyray.cms.model.CustomerProficient" parameterType="java.util.Map">
		select * from crm_customer_proficient order by id desc
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
	
	<select id="query" resultType="me.flyray.cms.model.CustomerProficient" parameterType="java.util.Map">
		select * from crm_customer_proficient 
		 <trim prefix="WHERE" suffixOverrides="AND">
            <if test="customerNo != null and customerNo != ''">customer_no = #{customerNo} AND</if>
         </trim>
	</select>
	
 	<select id="queryTotal" resultType="int">
		select count(*) from crm_customer_proficient 
	</select>
	 
	<insert id="save" parameterType="me.flyray.cms.model.CustomerProficient">
		insert into crm_customer_proficient
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="customerNo != null and customerNo != ''">
				customer_no,
			</if>
			<if test="certPhotoFileNo != null and certPhotoFileNo != ''">
				cert_photo_file_no,
			</if>
			<if test="leval != null and leval != ''">
				leval,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="customerNo != null and customerNo != ''">
				#{customerNo,jdbcType=VARCHAR},
			</if>
			<if test="certPhotoFileNo != null and certPhotoFileNo != ''">
				#{certPhotoFileNo,jdbcType=VARCHAR},
			</if>
			<if test="leval != null and leval != ''">
				#{leval,jdbcType=VARCHAR},
			</if>
		</trim>
	</insert>
	 
	<update id="update" parameterType="me.flyray.cms.model.CustomerProficient">
		update crm_customer_proficient 
		
	</update>
	
	<delete id="delete">
		delete from crm_customer_proficient where customer_no = #{customerNo}
	</delete>
  
</mapper>